      const addSlashes = (str) => str.replace(/'/g, "\'").replace(/"/g, '\"');
      const tLangCode = window._initialTranslatorState.targetLangcode;
      const sLangKey = 'selectLang_source_' + window._initialTranslatorState.sourceLangcode;
